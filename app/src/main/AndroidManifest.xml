<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".featureRunner"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/dragon_launcher_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.JokesApp"
        tools:targetApi="31">
        <activity
            android:name=".fbTest"
            android:exported="false" />
        <activity
            android:name=".LoginAct"
            android:exported="false" />
        <activity
            android:name=".SignUpAct"
            android:exported="false" />
        <activity
            android:name=".Dashboard"
            android:exported="false" />
        <activity
            android:name=".ClickerGameAct"
            android:exported="false" />
        <activity
            android:name=".dadJokesAct"
            android:exported="false" />
        <activity
            android:name=".NumeralConverterAct"
            android:exported="false" />
        <activity
            android:name=".MainAct"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>